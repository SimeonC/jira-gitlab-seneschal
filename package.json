    "helmet": "4.1.1",
    "html-webpack-plugin": "2.29.0",
    "lodash": "4.17.20",
    "map-keys-deep-lodash": "1.2.3",
    "morgan": "1.10.0",
    "nocache": "2.1.0",
    "nodemon": "2.0.4",
    "object-assign": "4.1.1",
    "pg": "8.3.3",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.1.0",
    "prop-types": "15.7.2",
    "raf": "3.4.1",
    "react": "16.13.1",
    "react-composer": "5.0.1",
    "react-dev-utils": "5.0.2",
    "react-dom": "16.13.1",
    "react-intl": "3.12.0",
    "react-json-view": "1.19.1",
