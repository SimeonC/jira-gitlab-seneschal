{
  "name": "jira-gitlab-seneschal",
  "version": "2.7.13",
  "private": true,
  "scripts": {
    "start": "node build/server/index.js",
    "start:dev": "AC_OPTS=force-dereg,force-reg nodemon --exec babel-node -- src/server/index.js",
    "build:client": "node scripts/build.js",
    "build:server": "babel -s -d ./build/server ./src/server && babel --source-root ./src/server -s -o ./build/frontendPages.js ./src/frontendPages.js && rm ./build/server/migrations/*.map",
    "build": "npm run build:client && npm run build:server",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "jest": "NODE_ENV=test BABEL_ENV=test jest --env=jsdom",
    "test": "npm run jest -- --coverage",
    "test:watch": "npm run jest -- --watch",
    "release": "standard-version && git push --follow-tags origin master && conventional-github-releaser -p angular",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.jsx\"",
    "postinstall": "node scripts/fixDevTools.js"
  },
  "dependencies": {
    "@apollo/client": "3.2.1",
    "@atlaskit/avatar": "20.0.4",
    "@atlaskit/button": "15.0.0",
    "@atlaskit/css-reset": "6.0.1",
    "@atlaskit/dynamic-table": "14.1.2",
    "@atlaskit/form": "8.1.4",
    "@atlaskit/icon": "21.1.2",
    "@atlaskit/icon-object": "6.0.4",
    "@atlaskit/item": "12.0.2",
    "@atlaskit/lozenge": "10.0.4",
    "@atlaskit/page": "12.0.1",
    "@atlaskit/select": "13.0.4",
    "@atlaskit/spinner": "15.0.3",
    "@atlaskit/tabs": "12.0.4",
    "@atlaskit/textfield": "4.0.7",
    "@atlaskit/toggle": "11.0.1",
    "@atlaskit/tooltip": "17.0.1",
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
    "@gitbeaker/node": "28.0.2",
    "@graphql-tools/schema": "6.2.3",
    "apollo-server-express": "2.19.2",
    "atlassian-connect-express": "5.0.0",
    "autoprefixer": "9.8.6",
    "axios": "0.21.1",
    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-import-graphql": "2.7.0",
    "babel-preset-flow-node": "2.0.1",
    "babel-preset-react-app": "3.1.2",
    "babel-runtime": "6.26.0",
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "chalk": "4.1.0",
    "compression": "1.7.4",
    "concurrently": "5.3.0",
    "conventional-github-releaser": "3.1.5",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "css-loader": "0.28.7",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "emotion": "10.0.27",
    "errorhandler": "1.5.1",
    "eslint": "4.10.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.21.2",
    "express": "4.17.1",
    "express-graphql": "0.11.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "9.0.1",
